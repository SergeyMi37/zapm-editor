 .I $D(^%ZAPM.bs.BSbufB("K"_$G(%BS(3),$P)_$J_"u"_%BS(15),"FTR",i,5)) S l1=@$ZR,l2=$G(^(5,1)),@(BSR_"(BST,""Key"",i-2,3)")=l1,^(4)=l2
 .I $D(^%ZAPM.bs.BSbufB("K"_$G(%BS(3),$P)_$J_"u"_%BS(15),"COL",i,5)) S l1=@$ZR,l2=$G(^(5,1)),@(BSR_"(BST,""Key"",i-2,5)")=l1,^(6)=l2
